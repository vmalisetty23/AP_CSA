public class String1
{
    public static void main(String[] args) 
    {
        String str = "Example";
        String nums = str + 1 + 2 + 3 + 4 + 5;
        
        System.out.println(nums);
        
        String myString = (5 + 5) + " is ten";
        System.out.println(myString);
    }
   
}
